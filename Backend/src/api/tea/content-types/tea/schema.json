{
  "kind": "collectionType",
  "collectionName": "teas",
  "info": {
    "singularName": "tea",
    "pluralName": "teas",
    "displayName": "tea"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "tea_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tea-category.tea-category",
      "inversedBy": "teas"
    },
    "shortDescription": {
      "type": "string",
      "required": true
    },
    "brewingTime": {
      "type": "integer",
      "default": 3
    },
    "temperature": {
      "type": "integer",
      "default": 70
    },
    "dosage": {
      "type": "string",
      "default": "2-3g pour 250ml"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
